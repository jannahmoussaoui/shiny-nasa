library(tibble)
library(dplyr)
# Common parameters
common_params <- list(
input_type = c("y/n", "select", "text"),
required = TRUE
)
# Questions
questions <- c(
"Are you completing this as an individual or with a group?",
"What group are you a part of?",
"What secret alias would you like to use? This will be displayed on a graph.",
"Rank the item: Box of matches",
"Rank the item: Food Concentrate",
"Rank the item: 50 feet of nylon rope",
"Rank the item: Parachute silk",
"Rank the item: Portable heating unit",
"Rank the item: Two .45 caliber pistols",
"Rank the item: One case of dehydrated milk",
"Rank the item: Two 100 lb. tanks of oxygen",
"Rank the item: Stellar map",
"Rank the item: Self-inflating life raft",
"Rank the item: Magnetic compass",
"Rank the item: 20 liters of water",
"Rank the item: Signal flares",
"Rank the item: First aid kit, including injection needle",
"Rank the item: Solar-powered FM receiver-transmitter"
)
# Create data frames in a loop
df_list <- lapply(questions, function(question) {
data.frame(
question = question,
option = if (grepl("Rank the item:", question)) t(as.character(1:15)) else "e.g., Clark Kent",
input_id = tolower(gsub(" |:", "", question)),
dependence = NA,
dependence_value = NA,
common_params
) %>%
pivot_longer(cols = starts_with("option"), values_to = "option")
})
# Combine all data frames into a single data frame
df <- bind_rows(df_list)
# Print the result
print(df)
